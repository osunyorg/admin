<% content_for :title, Research::Journal.model_name.human(count: 2) %>

<% content_for :title_right do %>
  <%= create_link Research::Journal %>
<% end %>

<div class="d-flex justify-content-between mb-5">
  <%= render 'admin/application/components/lifecycle/pills',
              path_method: :admin_research_journals_path %>
  <%= render 'filters', current_path: admin_research_journals_path %>
</div>
<%= render 'admin/application/components/lifecycle/label' %>

<%= render 'admin/research/journals/list', journals: @journals %>
<%= paginate @journals %>
