# DO NOT EDIT THIS FILE BY HAND - IT WILL BE OVERWRITTEN BY OSUNY

baseURL: <%= @website.url %>

## OSUNY
osuny:
  website:
    id: <%= @website.id %>

## LANGUAGE
defaultContentLanguage: <%= @website.default_language.iso_code %>
defaultContentLanguageInSubdir: <%= @website.active_languages.many? %>

## CACHE
deployment:
  matchers:
    - pattern: "^.+\\.(woff2|woff|svg|ttf|otf|eot|js|css)$"
      cacheControl: "max-age=31536000, no-transform, public"
      gzip: true
    - pattern: "^.+\\.(png|jpg|jpeg|gif|webp)$"
      cacheControl: "max-age=31536000, no-transform, public"
      gzip: false
<% if @website.deuxfleurs_hosting %>
  ## DEUXFLEURS
  targets:
    - name: "production"
      URL: "s3://<%= @website.deuxfleurs_identifier %>?endpoint=garage.deuxfleurs.fr&s3ForcePathStyle=true&region=garage&awssdk=v1"
<% end %>
