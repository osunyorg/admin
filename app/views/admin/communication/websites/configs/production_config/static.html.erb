# DO NOT EDIT THIS FILE BY HAND - IT WILL BE OVERWRITTEN BY OSUNY

baseURL: <%= @website.url %>

## OSUNY
osuny:
  website:
    id: <%= @website.id %>

<% if @website.has_plausible? %>
## PARAMS
params:
  _merge: deep
  plausible: "<%= @website.plausible_site_domain %>"
<% end %>

## LANGUAGE
defaultContentLanguage: <%= @website.default_language.iso_code %>
defaultContentLanguageInSubdir: <%= @website.active_languages.many? %>
