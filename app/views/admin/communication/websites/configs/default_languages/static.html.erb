<%
localizations = @website.localizations
                        .published
                        .includes(:language)
                        .order("languages.iso_code")
localizations.each do |l10n|
  language = l10n.language
%>
<%= language.iso_code %>:
  title: >-
    <%= l10n.to_s %>
  contentDir: content/<%= language.iso_code %>
  languageCode: <%= language.iso_code %>
  languageName: <%= language.name %>
  buildFuture: true
  params:
<%= render 'admin/application/contact_details/static', about: l10n, depth: 2 %>
  taxonomies:
    administrator: administrators
    author: authors
    diploma: diplomas
    events_category: events_categories
    location: locations
    organizations_category: organizations_categories
    pages_category: pages_categories
    persons_category: persons_categories
    posts_category: posts_categories
    programs_category: programs_categories
    projects_category: projects_categories
    researcher: researchers
    teacher: teachers
    volume: volumes
<% end %>