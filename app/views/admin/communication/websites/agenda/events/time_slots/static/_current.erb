<%
hugo = l10n.hugo(@website)
if hugo.file.present?
%>
current_time_slot:
  permalink: "<%= hugo.permalink %>"
  path: "<%= hugo.path %>"
  slug: "<%= hugo.slug %>"
  file: "<%= hugo.file %>"
  start:
    date: "<%= time_slot.date&.iso8601 %>"
    time: "<%= time_slot.time %>"
    datetime: "<%= time_slot.datetime&.iso8601 %>"
<% if time_slot.end_date %>
  end:
    date: "<%= time_slot.end_date&.iso8601 %>"
    time: "<%= time_slot.end_time %>"
    datetime: "<%= time_slot.end_datetime&.iso8601 %>"
<% end %>
  status: "<%= time_slot.status %>"
<%= osuny_static_text 'place', l10n.place, depth: 2 %>
<%= render 'admin/communication/websites/agenda/static_add_to_calendar', l10n: l10n %>
<% end %>